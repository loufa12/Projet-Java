            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Label>
      <Button fx:id="pseudos2_validation" layoutX="316.0" layoutY="356.0" mnemonicParsing="false" onAction="#pseudos2_validation" prefHeight="31.0" prefWidth="119.0" text="Validation final">
